<Window x:Class="jeopardy.Scraper"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:jeopardy"
        mc:Ignorable="d"
        Title="Scraper" Height="167" Width="224">
    <Grid Margin="0,0,7,4">
        <ProgressBar x:Name="Progress" HorizontalAlignment="Left" Height="30" Margin="15,50,0,0" VerticalAlignment="Top" Width="185" RenderTransformOrigin="1.69,1"/>
        <Label x:Name="Label" Content="Label" HorizontalAlignment="Left" Margin="15,14,0,0" VerticalAlignment="Top" Width="185"/>
        <Button x:Name="Download" Content="Download" HorizontalAlignment="Left" Margin="15,95,0,0" VerticalAlignment="Top" Width="75" Click="Download_Click"/>
        <Button x:Name="Scrape" Content="Scrape" HorizontalAlignment="Left" Margin="120,95,0,0" VerticalAlignment="Top" Width="75" Click="Scrape_Click"/>

    </Grid>
</Window>
